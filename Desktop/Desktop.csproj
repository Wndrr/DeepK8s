    </Content>
    <Content Update="Pages\CertManager\Challenges\Details.razor">
      <Generator>MSBuild:RazorGenerateComponentDeclarationDesignTime</Generator>
    </Content>
    <Content Update="Pages\CertManager\ClusterIssuers\List.razor">
      <Generator>MSBuild:RazorGenerateComponentDeclarationDesignTime</Generator>
    </Content>
    <Content Update="Pages\CertManager\ClusterIssuers\Details.razor">
      <Generator>MSBuild:RazorGenerateComponentDeclarationDesignTime</Generator>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <_ContentIncludedByDefault Remove="Pages\StatefulSets\Details.razor" />
    <_ContentIncludedByDefault Remove="Pages\StatefulSets\List.razor" />
