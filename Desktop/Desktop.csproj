  <ItemGroup>
    <ProjectReference Include="..\CertManagerDefinitions\CertManagerDefinitions.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Pages\Network" />
    <Folder Include="Pages\Storage" />
    <Folder Include="Pages\Workloads" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Remove="Pages\Entities\StatefulSets\Details.razor" />
    <UpToDateCheckInput Remove="Pages\Entities\StatefulSets\List.razor" />
  </ItemGroup>
  <ItemGroup>
    <None Update="Shared\Layout.razor.css">
      <DependentUpon>Layout.razor</DependentUpon>
    </None>
    <None Update="Shared\Navigation.razor.css">
      <DependentUpon>Navigation.razor</DependentUpon>
    </None>
  </ItemGroup>
</Project>